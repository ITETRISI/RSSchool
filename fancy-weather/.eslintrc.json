{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"airbnb-base"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", 2],
		"no-tabs": 0,
		"no-console": 0,
		"no-alert": 0,
		"no-param-reassign": 0
	},
	"settings": {
		"import/ignore": [
		  "current-time"
		]
	}
}